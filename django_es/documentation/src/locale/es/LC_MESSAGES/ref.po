msgstr ""

# 904272f354fd4fae93464de28806fc2d
#: ../../../../1.8/docs/ref/databases.txt:899
msgid "`IBM DB2`_"
msgstr ""

# 160eb9a4d3da40b284b3946621a4b29a
#: ../../../../1.8/docs/ref/databases.txt:900
msgid "`Microsoft SQL Server`_"
msgstr ""

# 5cced44fc38d4a9d915e827e03dc07a3
#: ../../../../1.8/docs/ref/databases.txt:901
msgid "Firebird_"
msgstr ""

# e7ce8801a8e54ea4b87fea50d9f1adb6
#: ../../../../1.8/docs/ref/databases.txt:902
msgid "ODBC_"
msgstr ""

# b9a4e448da9049f289e21a8832f8660f
#: ../../../../1.8/docs/ref/databases.txt:903
msgid "ADSDB_"
msgstr ""

# e6b1477e4b694826b5601d774879f739
#: ../../../../1.8/docs/ref/databases.txt:905
msgid ""
"The Django versions and ORM features supported by these unofficial backends "
