msgid "CREATE TABLESPACE"
msgstr ""

# 9049ef52a56f4003b06acf5793dc7764
#: ../../../../1.8/docs/ref/databases.txt:729
msgid "DROP TABLESPACE"
msgstr ""

# a2d60dd62af84b2f8f03d4a1886ec365
#: ../../../../1.8/docs/ref/databases.txt:730
msgid "CREATE SESSION WITH ADMIN OPTION"
msgstr ""

# c0b1ea8ffeb448cfb536d27f05569561
#: ../../../../1.8/docs/ref/databases.txt:731
msgid "CREATE TABLE WITH ADMIN OPTION"
msgstr ""

# d04a67ccfd704ee7adf9803ce18906f4
#: ../../../../1.8/docs/ref/databases.txt:732
msgid "CREATE SEQUENCE WITH ADMIN OPTION"
msgstr ""

# 6858266bb2224a83a020d130814a115c
#: ../../../../1.8/docs/ref/databases.txt:733
msgid "CREATE PROCEDURE WITH ADMIN OPTION"
msgstr ""

# ed7725bec76347c8a60855ff69227ec9
#: ../../../../1.8/docs/ref/databases.txt:734
msgid "CREATE TRIGGER WITH ADMIN OPTION"
msgstr ""

# b1540add5f074141b2c411ac23a0d6f7
#: ../../../../1.8/docs/ref/databases.txt:736
msgid ""
"Note that, while the RESOURCE role has the required CREATE TABLE, CREATE "
"SEQUENCE, CREATE PROCEDURE and CREATE TRIGGER privileges, and a user granted"
