msgstr ""

# 6588efa99e6b42f2956ff135d4682603
#: ../../../../1.8/docs/ref/middleware.txt:114
msgid "The response has already set the ``Content-Encoding`` header."
msgstr ""

# 874bf6b4f20042ca9f84fec38779437d
#: ../../../../1.8/docs/ref/middleware.txt:116
msgid ""
"The request (the browser) hasn't sent an ``Accept-Encoding`` header "
"containing ``gzip``."
msgstr ""

# c0116d7387af49e79820aadd8420a996
