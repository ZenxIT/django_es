# 82eba0294678415390e17a67bef77d73
#: ../../../../1.8/docs/ref/databases.txt:721
msgid "CREATE TRIGGER"
msgstr ""

# 691864bf2dd74e08b055c15ccfc20d49
#: ../../../../1.8/docs/ref/databases.txt:723
msgid ""
"To run a project's test suite, the user usually needs these *additional* "
"privileges:"
msgstr ""

# 2585d0565b44442c8468e32a213be7bc
#: ../../../../1.8/docs/ref/databases.txt:726
msgid "CREATE USER"
msgstr ""

# 0c4b7eff1e3c4b689a0c958e7be451d9
#: ../../../../1.8/docs/ref/databases.txt:727
msgid "DROP USER"
msgstr ""

# c1700da0a9ea469b8932f086dde3a95d
