msgstr ""

# 8a872aaee2e84baca82dc13f50584a27
#: ../../../../1.8/docs/internals/howto-release-django.txt:407
msgid "``(1, 3, 0, 'alpha', 0)`` → \"1.3 pre-alpha\""
msgstr ""

# cfc2ce2a62ae465b9093016245dbd5d5
#: ../../../../1.8/docs/internals/howto-release-django.txt:409
